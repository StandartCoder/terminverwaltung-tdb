{
  "name": "@terminverwaltung/database",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm",
    "db:generate": "prisma generate",
    "db:push": "dotenv -e ../../.env -- prisma db push",
    "db:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "db:seed": "dotenv -e ../../.env -- tsx prisma/seed.ts",
    "studio": "dotenv -e ../../.env -- prisma studio",
    "lint": "eslint src prisma --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "6.0.1",
    "@terminverwaltung/auth": "workspace:*"
  },
  "devDependencies": {
    "@terminverwaltung/eslint-config": "workspace:*",
    "@terminverwaltung/typescript-config": "workspace:*",
    "dotenv-cli": "^8.0.0",
    "prisma": "6.0.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2"
  }
}
