{
  "name": "@terminverwaltung/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./constants": {
      "types": "./src/constants.ts",
      "import": "./dist/constants.js"
    },
    "./utils": {
      "types": "./src/utils.ts",
      "import": "./dist/utils.js"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "import": "./dist/errors.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/constants.ts src/utils.ts src/errors.ts --format esm",
    "lint": "eslint src --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@terminverwaltung/eslint-config": "workspace:*",
    "@terminverwaltung/typescript-config": "workspace:*",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3"
  }
}
